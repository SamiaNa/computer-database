<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>2.0</jsp-version>
	<short-name>mylib</short-name>
	<tag>
		<name>link</name>
		<tag-class>com.excilys.java.formation.tags.LinkTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>target</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>pageNumber</name>
			<required>true</required>
            <type>java.lang.Integer</type>
            <fragment>true</fragment>
		</attribute>
		<attribute>
			<name>pageSize</name>
			<required>true</required>
            <type>java.lang.Integer</type>
            <fragment>true</fragment>
		</attribute>
	</tag>
    <tag>
        <name>getpage</name>
        <tag-class>com.excilys.java.formation.tags.GetPageTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>page</name>
            <required>true</required>
            <type>com.excilys.java.formation.page.Page</type>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>pageToGet</name>
            <required>true</required>
            <type>java.lang.Integer</type>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>target</name>
            <required>true</required>
        </attribute>
    </tag>
   <tag>
        <name>prevpage</name>
        <tag-class>com.excilys.java.formation.tags.PrevPageTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>page</name>
            <required>true</required>
            <type>com.excilys.java.formation.page.Page</type>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>target</name>
            <required>true</required>
        </attribute>
   </tag>
   
   <tag>
        <name>pagination</name>
        <tag-class>com.excilys.java.formation.tags.PaginationTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>page</name>
            <required>true</required>
            <type>com.excilys.java.formation.page.Page</type>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>target</name>
            <required>true</required>
        </attribute>
   </tag>
</taglib>